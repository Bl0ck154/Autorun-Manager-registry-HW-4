<Window x:Class="sys4_registry_AutorunManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:sys4_registry_AutorunManager"
        mc:Ignorable="d"
		TextElement.FontSize="18"
        Title="MainWindow" Height="450" Width="600">
    <DockPanel>
		<ToolBar DockPanel.Dock="Top">
			<Separator/>
			<Button Click="ButtonAdd_Click">Add</Button>
			<Separator/>
			<Button Name="removeBtn" Click="removeBtn_Click">Remove</Button>
			<Separator/>
		</ToolBar>
		<ListView Name="myList" SelectionMode="Single"></ListView>
    </DockPanel>
</Window>
